<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration>
	<!-- 引入配置文件 -->
	<properties resource="DbConfig.properties" />
   
    <!-- 指定数据连接驱动jar地址 -->  
    <classPathEntry location="D:/mysql-connector-java-5.1.18.jar" />  
    
	<context id="mycontext">
		<jdbcConnection driverClass="${test.jdbc.driverClassName}" connectionURL="${test.jdbc.url}"
			userId="${test.jdbc.username}" password="${test.jdbc.password}" />
			
		<javaModelGenerator targetPackage="com.xypay.msg.model" targetProject="msgmgr" />
			
		<sqlMapGenerator targetPackage="com.xypay.msg.dao" targetProject="msgmgr" />
		<javaClientGenerator targetPackage="com.xypay.msg.dao"
			targetProject="msgmgr" type="XMLMAPPER" />
		<table schema="" tableName="msg_chan_inf">
<!-- 			<columnOverride column="???" property="???" /> -->
		</table>
	</context>
</generatorConfiguration>